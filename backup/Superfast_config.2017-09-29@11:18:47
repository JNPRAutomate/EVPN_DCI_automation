policy-options {
    policy-statement export-HOT-VLAN-to-COLD-VLAN {
        term 1 {
            from {
                protocol direct;
                route-filter 10.0.0.0/8 orlonger;
            }
            then accept;
        }
    }
    policy-statement export-lo0 {
        term 1 {
            from {
                route-filter 100.0.0.0/8 orlonger;
            }
            then accept;
        }
        term 2 {
            from {
                protocol local;
                route-filter 10.201.0.3/32 exact;
            }
            then accept;
        }
    }
    policy-statement load-balance {
        then {
            load-balance per-packet;
        }
    }
}
firewall {
    family ethernet-switching {
        filter PDU-BLOCK {
            interface-specific;
            term HSRP {
                from {
                    destination-mac-address {
                        00:00:0c:07:ac:00/40;
                        00:00:0c:9f:f0:00/36;
                    }
                }
                then {
                    discard;
                    count HSRP-BLOCK;
                }
            }
            term PVSTP {
                from {
                    destination-mac-address {
                        01:00:0c:cc:cc:cd/48;
                    }
                }
                then {
                    discard;
                    count PVSTP-BLOCK;
                }
            }
            term IEEE-STP {
                from {
                    destination-mac-address {
                        01:80:c2:00:00:00/48;
                    }
                }
                then {
                    discard;
                    count IEEE-STP-BLOCK;
                }
            }
            inactive: term TEST {
                from {
                    destination-mac-address {
                        00:12:01:00:00:1d/48;
                    }
                }
                then {
                    discard;
                    count TEST-MAC;
                }
            }
            inactive: term DHCP {
                from {
                    destination-port dhcp;
                }
                then {
                    discard;
                    count DHCP-BLOCK;
                }
            }
            term LAST {
                then {
                    accept;
                    count ACCEPTED;
                }
            }
        }
    }
}
switch-options {
    vtep-source-interface lo0.0;
    route-distinguisher 100.1.1.2:1;
    vrf-target {
        target:1:100;
        auto;
    }
}
